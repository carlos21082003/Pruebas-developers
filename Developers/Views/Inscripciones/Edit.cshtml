@model Developers.Models.Enrollment

<h2>Editar Inscripción</h2>

<form asp-action="Edit" asp-controller="Inscripciones">
    <input type="hidden" asp-for="EnrollmentId" />
    <input type="hidden" asp-for="InscripcionesId" />
    <input type="hidden" asp-for="StudentId" />

    @* <input type="hidden" asp-for="StudentId" />
    <input type="hidden" asp-for="Inscripciones" /> *@
    <div class="form-group">
        <label asp-for="PreTest" class="control-label"></label>
        <input asp-for="PreTest" class="form-control" />
        <span asp-validation-for="PreTest" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="PostTest" class="control-label"></label>
        <input asp-for="PostTest" class="form-control" />
        <span asp-validation-for="PostTest" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Guardar" class="btn btn-primary" />
    </div>
</form>

<div>
    <a asp-action="Index">Volver a la lista</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
